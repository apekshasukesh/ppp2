#===================================================================================#

        for t in fnameFilter:
            vcf_filter.run(t)
            self.assertTrue(os.path.isfile(file_names[0]))
            self.assertTrue(os.path.isfile(file_names[0]+'.log'))     
        shutil.move(file_names[0], newpath[0])
        shutil.move(file_names[0]+'.log', newpath[0])

#===================================================================================#

